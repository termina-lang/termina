function change_flag (flag : &mut bool) {

    *flag = false;

    return;
}

function  fill_array_with_ones (data: &mut [u8;6]){

    let one : u8 = 1 : u8;
    var flag : bool = true;

    for i : u8 in 0 : u8 .. 6 : u8 while flag {

        (*data)[i as usize] = one;
        if (*data)[i as usize] != 1 : u8 {
            change_flag(&mut flag);
        }

    }

    return;
}
